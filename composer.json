{
    "name": "psr/event-dispatcher",
    "description": "Standard interfaces for event handling.",
    "type": "library",
    "keywords": ["psr", "psr-14", "events"],
    "license": "MIT",
    "authors": [
        {
            "name": "PHP-FIG",
            "homepage": "http://www.php-fig.org/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/container": "^1.0",
        "zendframework/zend-code": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "php-vfs/php-vfs": "*@stable"
    },
    "autoload": {
        "psr-4": {
            "Psr\\Event\\Dispatcher\\": "src/",
            "Crell\\EventDispatcher\\": "impl/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crell\\EventDispatcher\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
